{
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  },
  "main": "server/index.ts",
  "type": "module",
  "scripts": {
    "start": "node server/index.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
